		OWLNamedIndividual createInvidual = util
				.createInvidual("Kisi", "kisi1");
		util.addDataProperty("ad", "sinan", createInvidual);
		util.addDataProperty("ad", "kordemir", createInvidual);

		HashMap<String, List<String>> dataPropertiesWithList = util
				.getDataPropertiesWithList(createInvidual);
		List<String> list = dataPropertiesWithList.get("ad");
		String ad1 = list.get(0);
		String ad2 = list.get(1);
		if ((ad1.equals("sinan") || ad2.equals("sinan"))
				&& (ad1.equals("kordemir") || ad2.equals("kordemir")))
			assertTrue(true);
		else
			assertTrue(false);
	}

